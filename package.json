{
  "name": "university-course-progress-updater",
  "version": "2.2.4",
  "description": "Updates a README.md file on execution with a visualization of the latest progress state",
  "main": "updateProgress.js",
  "scripts": {
    "start": "node updateProgress.js",
    "dist": "node dist.js",
    "examples": "node examples/updateExamples.js",
    "updateJsonSchemaTsTypes": "ts-node jsonSchema2dts/createTsTypeFromJsonSchema.ts",
    "test": "mocha tests/**/*.test.js",
    "coverage": "nyc --reporter=lcov --reporter=text npm run test",
    "lint": "eslint -c .eslintrc.js --ext .js updateProgress.js updateProgressLib.js dist.js examples tests --fix"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^14.0.13",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsdoc": "^27.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "json-schema-to-typescript": "^9.1.0",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "sinon": "^9.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
